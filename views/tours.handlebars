                    </a>
                    <div class="card-content">
                        <div class="row align-items-center">
                            <div class="col-7">
                                <h6 class="black mb-2"><a href="/tours/{{../cc}}/{{name}}" target="_blank">{{displayName}}</a></h6>
                            </div>
                            <div class="col-5 align-middle">
                                <h6 class="primary-color text-right ">{{price}}</h6>
                            </div>
                            <div>
                                {{#each labels}}
                                <a class="btn btn-primary px-3 ml-3 mr-1 my-1 btn-sm float-left" href="#" role="button">{{this}}</a>
                                {{/each}}
                            </div>
                        </div>
                    </div>
                </div>
                
                {{#if @last}} 
                    <div class="row mb-lg-0 mb-4 float-right">
                    <div class="col-12 mt-3">
                        <div class='pagination'>
                            <div class='current'>
                                <h6>1</h6>
                            </div>
                            <a class="" href="#">
                                <h6>2</h6>
                            </a>
                            <a href="#">
                                <h6>3</h6>
                            </a>
                            <a href="#">
                                <h6 class="arrow-offset">›</h6>
                            </a>
                            <a href="#">
                                <h6 class="arrow-offset">»</h6>
                            </a>
                        </div>
                    </div>
                </div>
                {{/if}}




            </div>
            {{/each}}




